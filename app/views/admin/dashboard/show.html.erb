<%= ui_header t('page.titles.dashboard'), style: :h2 %>  

<% data = dashboard_admin_activity_counts %>
<%= ui_chart 'admin_activity_counts', 
      'Gauge', data[:counts], { height: 250, max: (data[:max] / 100.0).ceil * 100 }, 250 %>

<%= ui_header t('page.titles.user_activities'), :style => :h3 %>  
<%= ui_chart 'admin_user_activities', 
      'LineChart', 
      dashboard_admin_user_activities(3.weeks.ago),
      {legend: { position: 'top' }, chartArea: { left:50,top:40,width:'100%',height:'75%' }}, 400 %>

<%= ui_header t('page.titles.confirmations_vs_enrollments'), :style => :h3 %>  
<%= ui_chart 'user_enrollment_scatter', 
      'ScatterChart', 
      dashboard_confirmations_vs_enrollments(3.weeks.ago, 'confirmations', 'enrollments'),{
        hAxis: {title: t('page.titles.confirmation'), minValue: 0, maxValue: 15},
        vAxis: {title: t('page.titles.enrollment'), minValue: 0, maxValue: 15},
        legend: 'none', chartArea: { left:50,top:40,width:'100%',height:'75%' }
      }, 400 %> 