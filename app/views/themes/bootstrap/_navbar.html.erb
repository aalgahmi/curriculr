<% style = action_name == 'front' && current_user.blank? ? "navbar-custom" : "navbar-inverse" %>
<nav class="navbar yamm <%= style %> navbar-fixed-top" role="navigation">
	<div class="<%= css_container %>">
		<div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</button>
			<%= link_to main_app.root_path, :class => "navbar-brand" do %>
        <%= image_tag logo_path(current_account.config['logo'], :light), :height => 30 %>
      <% end %>
    </div>
		<div class="navbar-collapse collapse">
			<%= render "/ui_main_menu" %>
		</div>
	</div>
</nav>

