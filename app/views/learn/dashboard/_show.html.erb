<div class="row">
  <div class="<%= css_columns(9) %>">
    <% data = dashboard_klass_activity_counts(klass) %>
    <%= ui_chart 'klass_activity_counts', 
      'PieChart', data[:counts], {legend: 'none'}, 300 %>
  </div>
  <div class="<%= css_columns(15) %>">
   <%= ui_chart 'klass_daily_activities', 
     'LineChart', 
     dashboard_klass_activities_by_day(2.weeks.ago, klass),
     {curveType: 'function', legend: { position: 'top' }, chartArea:{left:0,top:30,width:'100%',height:'80%'}}, 300 %>
  </div>
</div>
